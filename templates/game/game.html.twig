{% extends 'base.html.twig' %}

{% block body %}
    <script>
        {% if user is not null %}
            window.user = {{ user|raw }};
        {% endif %}
    </script>
    <div id="Game"></div>
{% endblock %}

{% block javascripts %}
    {{ encore_entry_script_tags('game') }}
{% endblock %}

{% block stylesheets %}
    {{ encore_entry_link_tags('game') }}
{% endblock %}


